                                <div class="row" width="100%">
                                        <div class="col-md-2" width="100%">
                                                <% if (product.image) { %>
                                                        <a id="product-link" href="products/<%= product._id%>" class="nounderline"><img
                                                                        class="img-fluid" width="100%" height="auto"
                                                                        src="<%= product.image %> "></a>
                                                        <% } %>
                                        </div>
                                        <div class="col-md-8" width="100%">
                                                <div class="card-body" width="100%">
                                                        <h5 class="card-title"><a href="products/<%= product._id%>"
                                                                        class="nounderline">
                                                                        <%= product.title%>
                                                                </a></h5>
                                                        <p>
                                                                <% for (let cat of product.category) {%>
                                                                        <%= cat %>
                                                                                <% if (product.category.indexOf(cat)!==product.category.length
                                                                                        - 1) { %><small
                                                                                                class="text-muted">,&nbsp;
                                                                                        </small>
                                                                                        <% } %>
                                                                                                <% } %>
                                                        </p>
                                                        <p class="card-text">
                                                                <small class="text-muted">Διαθέσιμο σε:&nbsp;</small>
                                                                <% for (let s of product.price) { %>
                                                                        <small class="text-muted">
                                                                                <%= s.shop.organization%>
                                                                                        <% if (product.price.indexOf(s)!==product.price.length
                                                                                                - 1) { %><small
                                                                                                        class="text-muted">,&nbsp;
                                                                                                </small>
                                                                                                <% } %>
                                                                        </small>
                                                                        <% } %>
                                                        </p>
                                                        <br>
                                                        <br>
                                                        <br>
                                                        <br>
                                                        <p class="card-text">
                                                                <small class="text-muted">Η φωτογραφία ανήκει στην
                                                                        εταιρεία: <%= product.imageFrom %> </small>
                                                        </p>


                                                </div>


                                        </div>
                                        <div class="col-md-2 mt-4" width="100%">
                                                <a class="btn btn-primary btn-lg btn-block"
                                                        href="products/<%= product._id%>">Δείτε Προϊόν</a>
                                        </div>

                                </div>
                        </div>
                        <% } %>
